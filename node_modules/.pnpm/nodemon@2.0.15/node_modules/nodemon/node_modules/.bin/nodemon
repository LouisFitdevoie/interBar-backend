#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/node_modules:/Users/louisfitdevoie/Documents/Ecole/node_modules:/Users/louisfitdevoie/Documents/node_modules:/Users/louisfitdevoie/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/bin/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules/nodemon/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/nodemon@2.0.15/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules/.pnpm/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/interBar-backend/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/interBar/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/TFE/node_modules:/Users/louisfitdevoie/Documents/Ecole/2021-2022/node_modules:/Users/louisfitdevoie/Documents/Ecole/node_modules:/Users/louisfitdevoie/Documents/node_modules:/Users/louisfitdevoie/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
